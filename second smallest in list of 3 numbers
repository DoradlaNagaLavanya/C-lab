using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication2
{
    class Program
    {

        static void Main(string[] args)
        {

            int a = Convert.ToInt32(Console.ReadLine());
            int b = Convert.ToInt32(Console.ReadLine());
            int c = Convert.ToInt32(Console.ReadLine());
            if (a < b && a < c)
            {

                if (b < c)
                    Console.WriteLine(b);
                else
                    Console.WriteLine(c);

            }
            else if (b < a && b < a)
            {
                if (a < c)
                    Console.WriteLine(a);
                else
                    Console.WriteLine(c);
            }
            else
            {
                if (a < b)
                    Console.WriteLine(a);
                else
                    Console.WriteLine(b);
            }
        }
    }
}
